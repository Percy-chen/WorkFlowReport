<mvc:View
	controllerName="report.WorkFlowReport.controller.App"
	displayBlock="true"
	height="100%"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc">
	<App 
		id="app"
		busy="{/appProperties/busy}"
		busyIndicatorDelay="0">
		<f:FlexibleColumnLayout
			id="layout"
			layout="{/appProperties/layout}"
			backgroundDesign="Translucent">
		</f:FlexibleColumnLayout>
	</App>
</mvc:View>